<h1>Edit '<%= @sequence.name %>'</h1>

<form action="/sequences/<%= @sequence.slug %>/edit" method="post">
<input type="hidden" id="hidden" name="_method" value="patch">

  <label for="name">Name</label>
  <input type="text" id="name" name="name" value="<%= @sequence.name %>">

  <label for="name">Notes</label>
  <textarea id="name" name="notes"><%= @sequence.notes %></textarea>

  <button>Submit</button>

</form>